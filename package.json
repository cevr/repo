{
  "name": "@cvr/repo",
  "version": "1.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/cevr/repo.git"
  },
  "bin": {
    "repo": "./bin/repo"
  },
  "files": [
    "src",
    "tsconfig.json"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "bun build src/main.ts --outfile bin/repo --compile",
    "postinstall": "bun run build",
    "dev": "bun run src/main.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check",
    "gate": "concurrently -n type,lint,fmt,test -c blue,yellow,magenta,green \"bun run typecheck\" \"bun run lint:fix\" \"bun run fmt\" \"bun run test\"",
    "prepare": "lefthook install || true && effect-language-service patch",
    "link": "ln -sf $(pwd)/bin/repo ~/.bun/bin/repo",
    "version": "changeset version",
    "release": "changeset publish"
  },
  "dependencies": {
    "@effect/cli": "^0.73.1",
    "@effect/platform": "^0.94.2",
    "@effect/platform-bun": "^0.87.1",
    "effect": "^3.19.15"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.0",
    "@effect/language-service": "^0.72.0",
    "@effect/vitest": "^0.27.0",
    "@types/bun": "^1.3.6",
    "concurrently": "^9.2.1",
    "lefthook": "^2.0.15",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
